{
  "name": "vscode-pylens",
  "displayName": "pylens",
  "description": "Shows the latest version of a dependency in pyproject.toml or requirements.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/ninoseki/vscode-pylens.git"
  },
  "version": "0.1.2",
  "author": "Manabu Niseki",
  "publisher": "ninoseki",
  "license": "MIT",
  "engines": {
    "vscode": "^1.69.0"
  },
  "keywords": [
    "pyproject",
    "python"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:pyproject.toml",
    "workspaceContains:requirements.txt"
  ],
  "main": "./dist/extension.js",
  "icon": "images/logo.png",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/extension.ts --external vscode",
    "lint": "eslint src --ext ts",
    "dev": "npm run build --watch",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/jest": "28.1.6",
    "@types/node": "18.0.6",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "coveralls": "3.1.1",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.7",
    "tsup": "^6.1.3",
    "typescript": "4.7.4",
    "vscode-test": "1.6.1"
  },
  "dependencies": {
    "@sgarciac/bombadil": "^2.3.0",
    "axios": "^0.21.2",
    "axios-cache-adapter": "^2.7.3",
    "camelcase-keys": "^7.0.2",
    "toml": "^3.0.0"
  }
}

{
  "name": "vscode-pylens",
  "displayName": "pylens",
  "icon": "images/logo.png",
  "description": "Shows the latest version of a dependency in pyproject.toml or requirements.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/ninoseki/vscode-pylens.git"
  },
  "version": "0.0.3",
  "author": "Manabu Niseki",
  "publisher": "ninoseki",
  "license": "MIT",
  "engines": {
    "vscode": "^1.52.0"
  },
  "keywords": [
    "pyproject",
    "python"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:pyproject.toml",
    "workspaceContains:requirements.txt"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info",
    "format": "prettier --write src/*.ts src/**/*.ts"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "14.14.23",
    "@types/vscode": "1.52.0",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "coveralls": "3.1.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3",
    "vscode-test": "1.4.1"
  },
  "dependencies": {
    "@sgarciac/bombadil": "^2.3.0",
    "axios": "^0.21.1",
    "camelcase-keys": "^6.2.2",
    "toml": "^3.0.0"
  }
}
